<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import dk.gtz.graphedit.view.*?>
<BorderPane
    fx:id="root"
    xmlns:fx="http://javafx.com/fxml"
    xmlns="http://javafx.com/javafx"
    fx:controller="dk.gtz.graphedit.view.preloader.GraphEditPreloaderController">
<top>
</top>
<left>
    <BorderPane>
        <top>
            <VBox>
                <Button fx:id="newProjectButton"/>
                <Button fx:id="openNewProjectButton"/>
            </VBox>
        </top>
        <center>
            <VBox fx:id="projectContainer"/>
        </center>
    </BorderPane>
</left>
<center>
    <ImageView fx:id="logo"/>
</center>
<bottom>
    <HBox fx:id="bottomContainer">
        <ProgressBar fx:id="progressBar" progress="-1"/>
        <Label fx:id="statusLabel" text="waiting"/>
    </HBox>
</bottom>
</BorderPane>

